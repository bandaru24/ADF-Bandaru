{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfedwdev"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage23_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage23'"
		},
		"AzureBlobStorage24_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage24'"
		},
		"AzureBlobStorage25_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage25'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"EMPLKS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'EMPLKS'"
		},
		"Ifcondition_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ifcondition'"
		},
		"Prasad23_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Prasad23'"
		},
		"AzureBlobStorage26_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage26'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage23_connectionString": {
						"value": "[parameters('AzureBlobStorage23_connectionString')]"
					},
					"AzureBlobStorage24_connectionString": {
						"value": "[parameters('AzureBlobStorage24_connectionString')]"
					},
					"AzureBlobStorage25_connectionString": {
						"value": "[parameters('AzureBlobStorage25_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"EMPLKS_connectionString": {
						"value": "[parameters('EMPLKS_connectionString')]"
					},
					"Ifcondition_connectionString": {
						"value": "[parameters('Ifcondition_connectionString')]"
					},
					"Prasad23_connectionString": {
						"value": "[parameters('Prasad23_connectionString')]"
					},
					"AzureBlobStorage26_connectionString": {
						"value": "[parameters('AzureBlobStorage26_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}